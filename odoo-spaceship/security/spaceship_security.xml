<?xml version='1.0' encoding='utf-8'?>
<odoo>
  <record id='group_spaceship_admin' model='res.groups'>
    <field name='name'>Spaceship / Admin</field>
  </record>
  <record id='group_spaceship_crew' model='ir.rule'>
    <field name='name'>Crew access permissions</field>
    <field name='perm_read' eval='1' />
    <field name='perm_write' eval='1' />
    <field name='perm_create' eval='1'/>
    <field name='perm_unlink' eval='0' />
    <field name='domain_force'>[('active','=','True')]</field>
    <field name="model_id" ref="model_odoo_spaceship" />
  </record>
</odoo>